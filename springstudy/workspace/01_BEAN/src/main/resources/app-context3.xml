<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


		<!--  List -->
		<bean class="com.koreait.ex01.bean03.ListBean" id="listBean">
			<property name="list">
				<list>
					<value>김밥</value>
					<value>떡볶이</value>
					<value>순대</value>
				</list>
			</property>	
		</bean>
		
		
		<!--  Set -->
		<bean class="com.koreait.ex01.bean03.SetBean" id="setBean">
			<property name="set">
				<set>
					<value>Java</value>
					<value>Java</value>
					<value>Jsp</value>
					<value>Jsp</value>
					<value>Spring</value>
					<value>Spring</value>
				</set>
			</property>
		</bean>
		
		<!-- Map -->
		
		<bean class="com.koreait.ex01.bean03.MapBean" id="mapBean">
			<property name="map">
				<map>
					<entry key="beginRecord" value="1"/>				
					<entry key="endRecord" value="10"/>				
				</map>
			</property>
		
		
		</bean>
</beans>
